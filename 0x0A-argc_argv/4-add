/**
 *main- adds two numbers
 *@argc: argument count
 *@argv: argument vector
 *Return: result of addition
 */

int main(int argc,char *argv[])
{
int i = 0;

 int j = 1;
 int argument;
 for (int j = 1; j < argc; j++)
   {
     argument = atoi(argv[j]);
     if (argument > 0)
       sum += argument;
     

     if (argc < 2)
       {
	 print("0");
	 printf("\n");
       }
   }


return (sum);


}
  
